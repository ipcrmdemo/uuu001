{
  "family": "uuu001",
  "requiresCompatibilities":["EC2"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "0.5GB",
  "containerDefinitions": [ 
      {
      "portMappings": [
        {
          "hostPort": 8080,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "image": "registry.hub.docker.com/ipcrm/uuu001:0.1.0-SNAPSHOT-master.20181125122002",
      "healthCheck": {
        "retries": 3,
        "command": [
          "CMD-SHELL",
          "wget -O /dev/null http://localhost:8080 || exit 1"
        ],
        "timeout": 5,
        "interval": 30,
        "startPeriod": 30
      },
      "essential": true,
      "name": "uuu001"
    }
   ]
}
